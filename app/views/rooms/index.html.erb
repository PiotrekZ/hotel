<h1>Listing rooms</h1>

<table>
  <tr>
    <th>Typ</th>
    <th>Cena</th>
    <th>Lazienka</th>
    <th>Aneks kuchenny</th>
    <th>Minibarek</th>
    <th>Sejf</th>
    <th>Telewizor</th>
    <th>Telefon</th>
    <th>Radio</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @rooms.each do |room| %>
  <tr>
    <td><%= room.typ %></td>
    <td><%= room.cena %></td>
    <td><%= room.lazienka %></td>
    <td><%= room.aneks_kuchenny %></td>
    <td><%= room.minibarek %></td>
    <td><%= room.sejf %></td>
    <td><%= room.telewizor %></td>
    <td><%= room.telefon %></td>
    <td><%= room.radio %></td>
    <td><%= link_to 'Show', room %></td>
    <td><%= link_to 'Edit', edit_room_path(room) %></td>
    <td><%= link_to 'Destroy', room, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Room', new_room_path %>
